{
  "toMove": [
    "app/assets/favicon*",
    "!app/assets/**/*.html",
    "app/**/*.html",
    "app/assets/fonts/*"
  ],
  "injectDev": {
    "site": [
      "bower_components/normalize-css/normalize.css",
      "bower_components/flickity/dist/flickity.css",
      "app/assets/css/style.css",

      "bower_components/flickity/dist/flickity.pkgd.js",
      "bower_components/imagesloaded/imagesloaded.pkgd.js",
      "bower_components/scroll-up-bar/dist/scroll-up-bar.js",
      "bower_components/waypoints/lib/jquery.waypoints.js",
      "node_modules/barba.js/dist/barba.js",
      "app/assets/js/app.js",
      "app/assets/js/v-detect.js",
      "app/assets/js/v-device.js",
      "app/assets/js/v-dropdown.js",
      "app/assets/js/v-map.js",
      "app/assets/js/v-notify.js",
      "app/assets/js/v-toggle.js",
      "app/assets/js/v-tabs.js",
      "app/assets/js/v-interactions.js",
      "app/assets/js/v-flickity.js",
      "app/assets/js/v-waypoints.js",
      "app/assets/js/v-scrollto.js",
      "app/assets/js/v-barba.js"
    ]
  },
  "injectDist": {
    "site": [
      "dist/assets/css/style-min.css",
      "dist/assets/js/site-min.js"
    ]
  }
}
