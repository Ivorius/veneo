      "bower_components/waypoints/lib/shortcuts/sticky.js",
      "app/assets/js/app.js",
      "app/assets/js/v-detect.js",
      "app/assets/js/v-device.js",
      "app/assets/js/v-toggle.js",
      "app/assets/js/v-tabs.js",
      "app/assets/js/v-interactions.js",
      "app/assets/js/v-flickity.js",
      "app/assets/js/v-waypoints.js",
