block template-variables
  - var page = {title: "veneo", description: ""}

doctype
html.no-js(lang="en")
  head
    block template-head
      meta(charset="utf-8")
      meta(http-equiv="X-UA-Compatible", content="IE=edge, chrome=1")
      title!= page.title
      meta(name="description", content=page.description)
      meta(name="viewport", content="width=device-width, initial-scale=1")
      link(rel="icon", href="assets/favicon.ico")

      <!-- build:css assets/css/style-min.css -->
      <!-- site:css -->
      <!-- endinject -->
      <!-- endbuild -->

  body
    include ../mixins/_dummy
    include ../mixins/_header
    include ../mixins/_product-thumb
    include ../mixins/_cat-selector
    include ../mixins/_input-radio
    include ../mixins/_input-dropdown

    #barba-wrapper
      .barba-container
        block template-content
          p Basic template.

    block template-foot
      script(src="http://code.jquery.com/jquery-latest.min.js")

      <!-- build:js assets/js/site-min.js -->
      <!-- site:js -->
      <!-- endinject -->
      <!-- endbuild -->

      script(src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDO1mxHSkTP4u0_G5oWJOlPscRT7NIpINE&callback=veneoMap", defer, async)
